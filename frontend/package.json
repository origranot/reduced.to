{
  "name": "reduced.to-frontend",
  "description": "",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build --ssr src/entry.express.tsx",
    "build.static": "vite build --ssr src/entry.static.tsx",
    "build.types": "tsc --incremental --noEmit",
    "build:ssg": "npm run build && npm run ssg",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node server/entry.express",
    "ssg": "node server/entry.static",
    "ssg:dev": "nodemon -e ts,tsx -w ./src --exec \"npm run build:ssg\"",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.10.0",
    "@builder.io/qwik-city": "0.0.111",
    "@builder.io/qwik-react": "0.0.102",
    "@emotion/react": "11.9.3",
    "@emotion/server": "11.4.0",
    "@emotion/styled": "11.9.3",
    "@mui/material": "5.8.6",
    "@tailwindcss/typography": "^0.5.7",
    "@types/canvas-confetti": "^1.4.3",
    "@types/eslint": "8.4.6",
    "@types/express": "4.17.13",
    "@types/node": "latest",
    "@types/qrcode": "^1.5.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "autoprefixer": "^10.4.12",
    "eslint": "8.23.1",
    "eslint-plugin-qwik": "0.10.0",
    "express": "4.17.3",
    "node-fetch": "3.2.10",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.17",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.1.8",
    "typescript": "4.8.3",
    "vite": "3.1.1",
    "vite-tsconfig-paths": "3.5.0"
  },
  "dependencies": {
    "axios": "^1.1.2",
    "canvas-confetti": "^1.5.1",
    "daisyui": "^2.31.0",
    "qrcode": "^1.5.1"
  }
}
